{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "CommonJS",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": "./src",
    "lib": [
      "ES6"
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "./src/**/*",
    "./src/types/*.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}